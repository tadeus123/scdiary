<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
  <title>corner</title>
  <link rel="stylesheet" href="/css/style.css">
</head>
<body class="corner-page">
  <a href="/" class="corner-btn-left" aria-label="Back to Diary"></a>
  <a href="/bookshelf" class="bookshelf-btn" aria-label="Bookshelf">
    <svg class="bookshelf-icon" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round">
      <line x1="3" y1="7" x2="21" y2="7"></line>
      <line x1="3" y1="13" x2="21" y2="13"></line>
      <line x1="3" y1="19" x2="21" y2="19"></line>
      <line x1="3" y1="7" x2="3" y2="19"></line>
      <line x1="21" y1="7" x2="21" y2="19"></line>
    </svg>
  </a>
  
  <div class="corner-gallery">
    <div class="corner-image-stack" id="corner-image-stack">
      <!-- Images will be loaded dynamically -->
    </div>
  </div>
  
  <script src="/js/main.js"></script>
  <script src="/js/corner.js"></script>
</body>
</html>
